{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.8.1",
    "cors": "^2.8.5",
    "cron": "^2.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "next": "^13.4.4",
    "pg": "^8.11.0",
    "typeorm": "^0.3.16"
  },
  "scripts": {
